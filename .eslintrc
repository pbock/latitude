{
  "extends": [ "eslint:recommended", "plugin:react/recommended" ],
  "rules": {
    "comma-dangle": [2, "always-multiline"]
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [ "react", "babel" ]
}
